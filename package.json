{
  "name": "medusa-plugin-feeds",
  "version": "0.0.1",
  "description": "A Medusa plugin to provide product feeds.",
  "author": "Marco Freiberger <marco@freibergersyntax.dev>",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "clean": "cross-env ./node_modules/.bin/rimraf dist",
    "watch": "tsc --watch",
    "build": "cross-env pnpm run clean && pnpm run build:server && pnpm run build:admin",
    "build:server": "cross-env pnpm run clean && tsc -p tsconfig.json",
    "build:admin": "cross-env medusa-admin build",
    "prepare": "cross-env NODE_ENV=production pnpm run build:server && medusa-admin bundle",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "publishL": "yalc publish --push"
  },
  "dependencies": {
    "@medusajs/admin": "^7.0.1",
    "@medusajs/cache-redis": "^1.8.8",
    "@medusajs/event-bus-local": "^1.9.6",
    "@medusajs/event-bus-redis": "^1.8.9",
    "@medusajs/file-local": "^1.0.2",
    "@medusajs/ui": "^1.0.0",
    "@tanstack/react-query": "4.22.0",
    "babel-preset-medusa-package": "^1.1.13",
    "body-parser": "^1.19.1",
    "cors": "^2.8.5",
    "dotenv": "16.0.3",
    "express": "^4.17.2",
    "medusa-interfaces": "^1.3.7",
    "medusa-react": "^9.0.4",
    "node-product-catalog-feed": "^1.0.0",
    "prism-react-renderer": "^2.0.4",
    "react-xml-viewer": "^2.0.0",
    "typeorm": "^0.3.16",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-typescript": "^7.21.4",
    "@medusajs/medusa": "^1.15.0",
    "@medusajs/medusa-cli": "^1.3.16",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "@types/react": "^18.2.21",
    "babel-preset-medusa-package": "^1.1.13",
    "cross-env": "^7.0.3",
    "eslint": "^6.8.0",
    "fast-xml-parser": "^4.2.7",
    "jest": "^27.3.1",
    "medusa-core-utils": "^1.2.0",
    "mongoose": "^5.13.14",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.2"
  },
  "peerDependencies": {
    "@medusajs/medusa": "^1.15.0",
    "react": "^18.2.0",
    "react-router-dom": "^6.13.0"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.spec.json"
      }
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/node_modules/"
    ],
    "rootDir": "src",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js)$",
    "transform": {
      ".ts": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
